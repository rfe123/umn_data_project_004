        "\n",
        "# Determine play orientation - which team is to the right\n",
        "x_max = max(play_frame['x'])\n",
        "max_team = play_frame.loc[(play_frame['x'] == x_max)].iloc[0]['club']\n",
        "\n",
        "# Create end zone with team color and abbreviation\n",
