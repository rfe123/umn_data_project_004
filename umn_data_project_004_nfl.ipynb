   "source": [
    "# Conversion succes is accumulated over play progression and is based on if that type of formation is successful\n",
    "# as well as the total accumulation of all plays - not only 3rd down conversions\n",
    "# Success for the defense is a 0 in the 'conversion' column (indicating a blocked conversion)\n",
    "\n",
    "#initiate lists, dicts, and variables\n",
    "total_conversionDef_list = []\n",
    "conversionDef_list = []\n",
    "t=0\n",
    "\n",
    "# Fill nan values in 'offenseFormation' column with 'NONE'\n",
    "groupedDef_df['offenseFormation'] = groupedDef_df['offenseFormation'].fillna('NONE')\n",
    "\n",
    "formation_counts = {'SHOTGUN': 0, 'I_FORM': 0, 'SINGLEBACK': 0, 'EMPTY': 0, 'PISTOL': 0, 'WILDCAT': 0, 'JUMBO': 0, 'NONE':0}\n",
    "formation_successes = {'SHOTGUN': 0, 'I_FORM': 0, 'SINGLEBACK': 0, 'EMPTY': 0, 'PISTOL': 0, 'WILDCAT': 0, 'JUMBO': 0, 'NONE':0}\n",
    "\n",
    "\n",
    "for i in range(0, len(groupedDef_df['defensiveTeam'])):\n",
    "    \n",
    "    if groupedDef_df['defensiveTeam'][i] != team_list[t]:\n",
    "        t += 1\n",
    "        formation_counts = {formation: 0 for formation in formation_counts}\n",
    "        formation_successes = {formation: 0 for formation in formation_successes}\n",
    "        \n",
    "    formation = groupedDef_df['offenseFormation'][i]\n",
    "    formation_counts[formation] += 1\n",
    "    if groupedDef_df['conversion'][i] == 0: # conversion == 0 means it was blocked and a success by the defense\n",
    "        formation_successes[formation] += 1\n",
    "        \n",
    "    conversionDef_list.append(formation_successes[formation] / formation_counts[formation])\n",
    "    total_conversionDef_list.append(sum(formation_successes.values()) / sum(formation_counts.values()))\n",
    "\n",
    "# Lists created are placed into the dataframe    \n",
    "groupedDef_df['cumulativeConversionPerFormationDef'] = conversionDef_list\n",
    "groupedDef_df['cumulativeConversionTotalDef'] = total_conversionDef_list\n",
    "groupedDef_df.head()"
   ]
  },
