   "source": [
    "# Conversion succes is accumulated over play progression and is based on if that type of formation is successful\n",
    "# as well as the total accumulation of all plays - not only 3rd down conversions\n",
    "# Success for the offense is a 1 in the 'conversion' column\n",
    "\n",
    "#initiate lists, dicts, and variables\n",
    "total_conversionOff_list = []\n",
    "conversionOff_list = []\n",
    "t=0\n",
    "\n",
    "# Fill nan values in 'offenseFormation' column with 'NONE'\n",
    "groupedOff_df['offenseFormation'] = groupedOff_df['offenseFormation'].fillna('NONE')\n",
    "\n",
    "formation_counts = {'SHOTGUN': 0, 'I_FORM': 0, 'SINGLEBACK': 0, 'EMPTY': 0, 'PISTOL': 0, 'WILDCAT': 0, 'JUMBO': 0, 'NONE':0}\n",
    "formation_successes = {'SHOTGUN': 0, 'I_FORM': 0, 'SINGLEBACK': 0, 'EMPTY': 0, 'PISTOL': 0, 'WILDCAT': 0, 'JUMBO': 0, 'NONE':0}\n",
    "\n",
    "\n",
    "for i in range(0, len(groupedOff_df['possessionTeam'])):\n",
    "    \n",
    "    if groupedOff_df['possessionTeam'][i] != team_list[t]:\n",
    "        t += 1\n",
    "        formation_counts = {formation: 0 for formation in formation_counts}\n",
    "        formation_successes = {formation: 0 for formation in formation_successes}\n",
    "        \n",
    "    formation = groupedOff_df['offenseFormation'][i]\n",
    "    formation_counts[formation] += 1\n",
    "    if groupedOff_df['conversion'][i] == 1: # conversion == 1 means it was achieved and a success by the offense\n",
    "        formation_successes[formation] += 1\n",
    "        \n",
    "    conversionOff_list.append(formation_successes[formation] / formation_counts[formation])\n",
    "    total_conversionOff_list.append(sum(formation_successes.values()) / sum(formation_counts.values()))\n",
    "\n",
    "# Lists created are placed into the dataframe    \n",
    "groupedOff_df['cumulativeConversionPerFormationOff'] = conversionOff_list\n",
    "groupedOff_df['cumulativeConversionTotalOff'] = total_conversionOff_list\n",
    "groupedOff_df.head()"
   ]
  },
